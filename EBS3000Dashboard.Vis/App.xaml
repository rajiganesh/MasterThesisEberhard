<Application x:Class="EBS3000Dashboard.Vis.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EBS3000Dashboard.Vis"
             Exit="Application_Exit" ShutdownMode="OnLastWindowClose">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Eberhard.Core;component/Themes/Generic.xaml" />
        <ResourceDictionary Source="pack://application:,,,/EBS3000Dashboard.Vis;component/ViewMap.xaml" />
        <ResourceDictionary Source="DashboardStyle.xaml"/>
        <ResourceDictionary Source="Resources/lang.de-DE.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
